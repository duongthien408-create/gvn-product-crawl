{
  "name": "cellphones-crawler",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler pages dev ./public --d1 DB",
    "deploy": "wrangler pages deploy ./public",
    "db:create": "wrangler d1 create cellphones-db",
    "db:migrate": "wrangler d1 execute cellphones-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute cellphones-db --local --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "wrangler": "^3.0.0"
  }
}
